generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"  // pode ser postgresql, mysql etc.
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
  password String @default("abc") // jรก havia registros antes de inserir a senha
}
